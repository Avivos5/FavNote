iv`\n    width: 400px;\n    height: 400px;\n    background-color: white;\n    border-radius: 10px;\n    box-shadow: 0 10px 20px -10px rgba(0, 0, 0, 0.2);\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n`;\n\nconst StyledForm = styled(Form)`\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n`;\n\nconst StyledInput = styled(Input)`\n    margin: 0 0 30px 0;\n    height: 40px;\n    width: 300px;\n`;\n\nconst StyledHeading = styled(Heading)`\n    margin-bottom: 25px;\n`;\n\nconst StyledLink = styled(Link)`\n    display: block;\n    font-size: ${({ theme }) => theme.fontSize.s};\n    font-weight: ${({ theme }) => theme.bold};\n    color: black;\n    text-transform: uppercase;\n    margin: 20px 0 50px;\n`;\n\nconst AuthTemplate = ({ authType, authenticateLogin, userID }) => {\n    return (\n        <StyledWrapper>\n            <StyledLogo src={logo} alt=\"FavNote logo\" />\n            <Heading>Your new favorite online notes experience</Heading>\n            <AuthCard>\n                <Formik\n                    initialValues={{ username: '', password: '' }}\n                    onSubmit={({ username, password }) => {\n                        console.log(username, password);\n                        switch (authType) {\n                            case 'login':\n                                authenticateLogin(username, password);\n                                break;\n\n                            default:\n                                break;\n                        }\n                    }}>\n                    {({ handleChange, handleBlur, values }) => {\n                        if (userID) {\n                            return <Redirect to={routes.home} />; //żeby nie było teogo błędu podczas logowania z niezaładowanym pageContext, tutaj musi być routes.notes. Narazie zosawiam tak, ale warto to sprawdzić\n                        }\n                        return (\n                            <>\n                                <StyledHeading>\n                                    {authType === 'login' ? 'Sign in' : 'Registeration'}\n                                </StyledHeading>\n                                <StyledForm>\n                                    <StyledInput\n                                        placeholder=\"login\"\n                                        type=\"text\"\n                                        name=\"username\"\n                                        onChange={handleChange}\n                                        onBlur={handleBlur}\n                                        value={values.username}\n                                    />\n                                    <StyledInput\n                                        placeholder=\"password\"\n                                        type=\"password\"\n                                        name=\"password\"\n                                        onChange={handleChange}\n                                        onBlur={handleBlur}\n                                        value={values.password}\n                                    />\n                                    <Button activeColor=\"notes\" type=\"submit\">\n                                        {authType === 'login' ? 'Sign in' : 'Register'}\n                                    </Button>\n                                </StyledForm>\n                                <StyledLink\n                                    to={routes[authType === 'login' ? 'register' : 'login']}>\n                                    I want to {authType === 'login' ? 'Register' : 'Login in'}\n                                </StyledLink>\n                            </>\n                        );\n                    }}\n                </Formik>\n            </AuthCard>\n        </StyledWrapper>\n    );\n};\n\nconst mapStateToProps = ({ userID = null }) => ({\n    userID\n});\n\nconst mapDispatchToProps = (dispatch) => ({\n    authenticateLogin: (username, password) => dispatch(authenticateAction(username, password))\n});\n\nAuthTemplate.propTypes = {\n    authType: PropTypes.oneOf(['login', 'register'])\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(AuthTemplate);\n","/Users/mateuszgrabowski/Desktop/favnote/src/templates/DetailsTemplate.js",[],"/Users/mateuszgrabowski/Desktop/favnote/src/templates/GridTemplate.js",[],"/Users/mateuszgrabowski/Desktop/favnote/src/templates/UserPageTemplate.js",[],"/Users/mateuszgrabowski/Desktop/favnote/src/theme/GlobalStyle.js",[],"/Users/mateuszgrabowski/Desktop/favnote/src/theme/mainTheme.js",[],"/Users/mateuszgrabowski/Desktop/favnote/src/components/molecules/Card/Card.js",[],"/Users/mateuszgrabowski/Desktop/favnote/src/context/index.js",[],"/Users/mateuszgrabowski/Desktop/favnote/src/hoc/withContext.js",[],"/Users/mateuszgrabowski/Desktop/favnote/src/components/atoms/Paragraph/Paragraph.js",[],"/Users/mateuszgrabowski/Desktop/favnote/src/components/atoms/Button/Button.js",[],"/Users/mateuszgrabowski/Desktop/favnote/src/components/atoms/Heading/Heading.js",[],"/Users/mateuszgrabowski/Desktop/favnote/src/components/atoms/Input/Input.js",[],"/Users/mateuszgrabowski/Desktop/favnote/src/reducers/index.js",[],"/Users/mateuszgrabowski/Desktop/favnote/src/actions/index.js",[],"/Users/mateuszgrabowski/Desktop/favnote/src/components/atoms/ButtonIcon/ButtonIcon.js",[],"/Users/mateuszgrabowski/Desktop/favnote/src/components/organisms/NewItemBar/NewItemBar.js",[],"/Users/mateuszgrabowski/Desktop/favnote/src/components/organisms/Sidebar/Sidebar.js",[],{"ruleId":"124","replacedBy":"125"},{"ruleId":"126","severity":1,"message":"127","line":10,"column":20,"nodeType":"128","endLine":10,"endColumn":33},{"ruleId":"126","severity":1,"message":"129","line":73,"column":35,"nodeType":"130","endLine":73,"endColumn":52},{"ruleId":"126","severity":1,"message":"131","line":73,"column":54,"nodeType":"130",